{
  "name": "discord-bot-template",
  "version": "0.0.1",
  "author": "alexbcberio <alexbcberio@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "watch": "tsc && concurrently -r true \"yarn build:dev\" \"yarn start:dev\"",
    "start:dev": "cross-env NODE_ENV=development DEBUG=*,-nodemon* nodemon -r dotenv/config -r source-map-support/register --watch dist dist/index.js",
    "build": "yarn lint && rimraf dist && tsc",
    "build:dev": "tsc -w",
    "lint": "eslint src/**/*",
    "lint-fix": "yarn lint --fix"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "bufferutil": "^4.0.6",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "discord-api-types": "^0.27.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "erlpack": "discord/erlpack",
    "supports-color": "^9.2.1",
    "utf-8-validate": "^5.0.8",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "typescript": "4.4.4"
  }
}
